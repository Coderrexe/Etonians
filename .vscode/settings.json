{
    "python.pythonPath": "/usr/local/bin/python3.7",
    "git.decorations.enabled": true,
    "files.exclude": {
        "**/.DS_Store": true,
        "**/.pyc": {"when": "$(basename).py"},
        "**/__pycache__": true,
        "**/site.db": true,
        "**/venv": true,
        "**/profile_pictures": false,
        "**/images/": false
    },
    "python.linting.pylintEnabled": false,
    "editor.showUnused": true
}
