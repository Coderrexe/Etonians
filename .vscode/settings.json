{
    "python.pythonPath": "venv/bin/python",
    "git.decorations.enabled": true,
    "files.exclude": {
        "**/.DS_Store": true,
        "**/.pyc": {
            "when": "$(basename).py"
        },
        "**/__pycache__": true,
        "**/site.db": false,
        "**/venv": false,
        "**/user_images": false,
        "**/assets/": false,
        "**/dist/": true
    },
    "editor.showUnused": true,
    "typescript.suggest.paths": false,
}
